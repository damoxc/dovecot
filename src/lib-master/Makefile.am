	-I$(top_srcdir)/src/lib \
	-I$(top_srcdir)/src/lib-settings \
	-DSYSCONFDIR=\""$(sysconfdir)"\" \
	-DBINDIR=\""$(bindir)"\"

libmaster_la_SOURCES = \
	master-auth.c \
	master-service.c \
	master-service-settings.c \
	syslog-util.c

noinst_HEADERS = \
	master-auth.h \
	master-interface.h \
	master-service.h \
	master-service-private.h \
	master-service-settings.h \
	syslog-util.h
